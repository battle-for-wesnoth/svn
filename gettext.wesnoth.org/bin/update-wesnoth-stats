#!/bin/sh
cd $HOME/wesnoth/wesnoth-1.2/po
find . -name "*.po" -exec rm {} \;
svn cleanup
svn up

cd $HOME/wesnoth/wesnoth/po
find . -name "*.po" -exec rm {} \;
svn cleanup
svn up

cd $HOME/wesnoth/wescamp-i18n/
find . -name "*.po" -exec rm {} \;
svn cleanup
svn up
wget -q -O /dev/null http://pulsar.unizar.es/~isaac/wesnoth-gettext/westats/grab-stats.php
