#: data/campaigns/Legend_of_Wesmere/scenarios/16_The_Chief_must_die.cfg:133
msgid "Khrubar"
msgstr "Khrubar"

#. [side]: type=Orcish Warlord, id=Gvur
#: data/campaigns/Legend_of_Wesmere/scenarios/16_The_Chief_must_die.cfg:159
msgid "Gvur"
msgstr "Gvur"

