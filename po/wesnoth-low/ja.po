#. [unit]: type=Water Serpent, id=Sealurr
#: data/campaigns/Legend_of_Wesmere/scenarios/09_Bounty_hunters.cfg:434
msgid "Sealurr"
msgstr "Sealurr"

#. [message]: speaker=unit
#: data/campaigns/Legend_of_Wesmere/scenarios/09_Bounty_hunters.cfg:445
