#: data/help.cfg:10 data/help.cfg:52
msgid "Introduction"
msgstr "Introducción"

#: data/help.cfg:16
msgid "Gameplay"
msgstr "Juego"

#: data/help.cfg:22 data/help.cfg:203
msgid "Traits"
