# type: Plain text
#: ../../doc/man/wesnothd.6:266
msgid "B<wesnoth>(6), B<wesnoth_editor>(6)"
msgstr "B<wesnoth>(6), B<wesnoth_editor>(6)"

# type: Plain text
#~ msgid "start the in-game map editor directly."
#~ msgstr "start the in-game map editor directly."
