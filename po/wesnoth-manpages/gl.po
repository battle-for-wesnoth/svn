# type: Plain text
#: ../../doc/man/wesnoth.6:140
msgid ""
"skip menu, connect to specified host if any, otherwise connect to the first "
"server in preferences. Example: B<--server server.wesnoth.org>"
msgstr ""

# type: TP
#: ../../doc/man/wesnoth.6:140
#, no-wrap
msgid "B<-t, --test>"
msgstr "B<-t, --test>"

# type: Plain text
#: ../../doc/man/wesnoth.6:143
msgid "runs the game in a small test scenario."
msgstr "executa o xogo nun pequeno escenario de proba."

# type: TP
#: ../../doc/man/wesnoth.6:143
#, no-wrap
msgid "B<-v, --version>"
msgstr "B<-v, --version>"

# type: Plain text
