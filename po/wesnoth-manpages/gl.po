#, no-wrap
msgid "B<--bpp>I<\\ number>"
msgstr "B<--bpp>I<\\ número>"

# type: Plain text
#: ../../doc/man/wesnoth.6:49 ../../doc/man/wesnoth_editor.6:38
msgid "sets BitsPerPixel value. Example: B<--bpp 32>"
msgstr "establece o valor de BitsPerPixel. Exemplo: B<--bpp 32>"

