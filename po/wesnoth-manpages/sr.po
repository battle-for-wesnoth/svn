# type: TP
#: ../../doc/man/wesnoth_editor.6:47
#, no-wrap
msgid "B<--log->levelB<=>domain1B<,>domain2B<,>..."
msgstr "B<--log->нивоB<=>домен1B<,>домен2B<,>..."

# type: Plain text
