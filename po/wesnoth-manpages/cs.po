"vladimir@seznam.cziE<gt>."

# type: Plain text
#: ../../doc/man/wesnothd.6:206
msgid "B<wesnoth>(6), B<wesnoth_editor>(6)"
msgstr "B<wesnoth>(6), B<wesnoth_editor>(6)"
