#, no-wrap
msgid "The general syntax is:"
msgstr "La sintaxis general es:"

# type: Plain text
#: ../../doc/man/wesnothd.6:101
msgid "[I<tag>]"
msgstr "[I<etiqueta>]"

# type: Plain text
#: ../../doc/man/wesnothd.6:103
msgid "key=\"I<value>\""
msgstr "clau=\"I<valor>\""

# type: Plain text
