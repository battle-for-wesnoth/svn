#: ../../doc/man/wesnoth.6:16
#, no-wrap
msgid "WESNOTH"
msgstr "WESNOTH"

# type: TH
#: ../../doc/man/wesnoth.6:16 ../../doc/man/wesnothd.6:16
#, no-wrap
msgid "2009"
msgstr "2009"

# type: TH
#: ../../doc/man/wesnoth.6:16
#, no-wrap
msgid "wesnoth"
msgstr "wesnoth"

# type: TH
#: ../../doc/man/wesnoth.6:16
