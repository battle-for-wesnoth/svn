#: data/campaigns/Northern_Rebirth/scenarios/08a_Elvish_Princess.cfg:174
#: data/campaigns/Northern_Rebirth/scenarios/09a_Introductions.cfg:23
#: data/campaigns/Northern_Rebirth/scenarios/07a_Settling_Disputes.cfg:194
#: data/campaigns/Northern_Rebirth/scenarios/07a_Settling_Disputes.cfg:211
#: data/campaigns/Northern_Rebirth/scenarios/07b_Protecting_the_Master.cfg:123
#, fuzzy
msgid "Knalgans"
msgstr "Knash"

#. [unit]: type=Orcish Sovereign
#. [side]: type=Orcish Sovereign
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:208
#: data/campaigns/Northern_Rebirth/scenarios/06a_Old_Friend.cfg:145
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:43
#: data/campaigns/Northern_Rebirth/scenarios/09b_Judgement.cfg:43
msgid "Rakshas"
msgstr "Rakshas"

#. [side]: type=Troll Warrior
#. [side]: type=Orcish Warlord
#. [side]: type=Orcish Sovereign
#. [side]
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:45
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:62
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:75
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:88
#: data/campaigns/Northern_Rebirth/scenarios/11a_Eastern_Flank.cfg:101
#: data/campaigns/Northern_Rebirth/scenarios/06a_Old_Friend.cfg:152
#: data/campaigns/Northern_Rebirth/scenarios/06a_Old_Friend.cfg:171
#: data/campaigns/Northern_Rebirth/scenarios/06a_Old_Friend.cfg:189
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:51
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:88
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:120
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:152
#: data/campaigns/Northern_Rebirth/scenarios/13a_Showdown.cfg:184
