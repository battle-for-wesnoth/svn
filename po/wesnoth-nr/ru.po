#~ msgstr "Сам"

#~ msgid "Marcus!"
#~ msgstr "Маркус!"

#~ msgid "Anita"
#~ msgstr "Анита"

#~ msgid "Tentacle"
#~ msgstr "Щупальце"

#~ msgid "Sea Serpent"
#~ msgstr "Морская Змея"
