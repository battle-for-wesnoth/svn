msgstr "wesnothd - Démon na sieťovú hru Bitky o B<Wesnoth>"

# type: Plain text
#: ../../doc/man/wesnothd.6:34
#, fuzzy
msgid ""
"B<wesnothd> [\\|B<-dvV>\\|] [\\|B<-c> I<path>\\|] [\\|B<-p> I<port>\\|] [\\|"
"B<-t> I<number>\\|]"
msgstr ""
"B<wesnothd> [\\|B<-dvV>\\|] [\\|B<-c> I<cesta>\\|] [\\|B<-p> I<port>\\|] [\\|"
