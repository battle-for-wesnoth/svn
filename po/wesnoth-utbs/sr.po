msgid "Goblin Scout"
msgstr "гобли-извиђач"

#. [message]: speaker=Goblin Scout
#: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg:765
msgid "Attack!"
msgstr "Напад!"

#. [message]: speaker=Goblin Coward
#: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg:771
