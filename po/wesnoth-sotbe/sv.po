msgid "Turns run out"
msgstr "Dragen tar slut"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Black_Flag.cfg:54
msgid "Transport Galleon"
msgstr "Transportgaljon"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Black_Flag.cfg:56
