#: data/campaigns/Son_Of_The_Black_Eye/scenarios/04_The_Siege_Of_Barag_Gor.cfg:159
msgid "Ammon"
msgstr "Ammon"

#. [side]: type=Elvish Marshal, description=Sammual
#: data/campaigns/Son_Of_The_Black_Eye/scenarios/04_The_Siege_Of_Barag_Gor.cfg:194
msgid "Sammual"
msgstr "Sammual"

#. [event]
#: data/campaigns/Son_Of_The_Black_Eye/scenarios/04_The_Siege_Of_Barag_Gor.cfg:276
#: data/campaigns/Son_Of_The_Black_Eye/scenarios/11_Clash_Of_Armies.cfg:439
msgid "Pirk"
msgstr "Pirk"

