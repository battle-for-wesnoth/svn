msgid "Turns run out"
msgstr "Fin de los turnos"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Black_Flag.cfg:54
msgid "Transport Galleon"
msgstr "Galeón de transporte"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Black_Flag.cfg:56
msgid "Transport"
msgstr "Transporte"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Black_Flag.cfg:66
#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Clash_Of_Armies.cfg:72
#: data/campaigns/Son_Of_The_Black_Eye/scenarios/Desert_Of_Death.cfg:49
