#: data/campaigns/The_Dark_Hordes.cfg:9
msgid "The Dark Hordes (Part 1)"
msgstr "Mroczne Hordy (Części 1)"

#: data/campaigns/The_Dark_Hordes.cfg:13
msgid "Apprentice"
msgstr "Nowicjusz"

#: data/campaigns/The_Dark_Hordes.cfg:13
msgid "(easiest)"
msgstr "(najprostszy)"

#: data/campaigns/The_Dark_Hordes.cfg:14
msgid "Master"
msgstr "Mistrz"

#: data/campaigns/The_Dark_Hordes.cfg:15
msgid "(hardest)"
msgstr "(najtrudniejszy)"

#: data/campaigns/The_Dark_Hordes.cfg:15
msgid "Dark Lord"
msgstr "Mroczny lord"

#: data/campaigns/The_Dark_Hordes.cfg:17
#, fuzzy
msgid ""
"Lead fugitive dark sorcerer Gwiti Ha'atel to mastery of the undead hordes."
msgstr ""
"Poprowadź zbiegłego nekromantę Gwitiego Ha'atela do panowania nad hordami "
"nieumarłych."

#: data/campaigns/The_Dark_Hordes/scenarios/A_New_Chance.cfg:4
msgid "A New Chance"
msgstr "Nowa szansa"

#: data/campaigns/The_Dark_Hordes/scenarios/A_New_Chance.cfg:30
msgid "Clear the land of monsters"
msgstr "Oczyść ląd z potworów"

#: data/campaigns/The_Dark_Hordes/scenarios/A_New_Chance.cfg:34
#: data/campaigns/The_Dark_Hordes/scenarios/Brother_Against_Brother.cfg:35
#: data/campaigns/The_Dark_Hordes/scenarios/Confrontation.cfg:29
#: data/campaigns/The_Dark_Hordes/scenarios/Confrontation.cfg:166
#: data/campaigns/The_Dark_Hordes/scenarios/Crelanus_Book.cfg:29
