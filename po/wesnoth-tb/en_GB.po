#: data/campaigns/Two_Brothers/scenarios/3_Guarded_Castle.cfg:330
msgid "$first_password_1|!"
msgstr ""

#. [option]: speaker=Arne
#: data/campaigns/Two_Brothers/scenarios/3_Guarded_Castle.cfg:336
