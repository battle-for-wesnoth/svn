msgid "Flixta"
msgstr "Flixta"

#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/20_Return_of_the_Fleet.cfg:489
msgid "Bzz'Kza"
msgstr "Bzz'Kza"

#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/20_Return_of_the_Fleet.cfg:492
