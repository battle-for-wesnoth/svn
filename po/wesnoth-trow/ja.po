# ##
# #chap
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:4
msgid "Troll Hole"
msgstr "トロルの穴"

# #unit
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:50
msgid "Erart"
msgstr "Erart"

# #unit
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:75
msgid "Raol"
msgstr "Raol"

