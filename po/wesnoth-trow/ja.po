msgstr "Oldwood への帰還"

# #unit
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/07_Return_to_Oldwood.cfg:35
msgid "Surprisingly-Treelike"
msgstr "Surprisingly-Treelike"

