#~ msgstr "on"

#~ msgid "she"
#~ msgstr "ona"

#~ msgid "Click on $student"
#~ msgstr "Kliknij na $student"

#~ msgid "I have enough gold to recruit more units!"
#~ msgstr "Mam dość złota, by rekrutować nowe jednostki!"

#~ msgid "Dummy"
#~ msgstr "Atrapa"

#~ msgid "Basic Training"
#~ msgstr "Trening Podstawowy"

#~ msgid "Hello, Konrad."
#~ msgstr "Witaj, Konradzie."

#~ msgid ""
#~ "I am here to learn how to fight in a battle. As an experienced elder "
#~ "mage, surely you can explain the art of combat to me..."
