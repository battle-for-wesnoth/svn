#: data/core/help.cfg:21
msgid "Traits"
msgstr "Karaktertræk"

#: data/core/help.cfg:30
msgid "Units"
msgstr "Enheder"

#: data/core/help.cfg:37
msgid "Abilities"
msgstr "Evner"

#: data/core/help.cfg:45
msgid "Weapon Specials"
msgstr "Våbenspecialiteter"

