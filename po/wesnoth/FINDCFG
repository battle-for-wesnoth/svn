find data -name themes -prune -o -name scenarios -prune -o -name factions -prune -o -name units -prune -o -name campaigns -prune -o -name '*.cfg' -print
find data/scenarios/multiplayer -name '*.cfg' -print
find data/campaigns/*/units -name '*.cfg' -print
find data/tutorial/units -name '*.cfg' -print
echo data/english.cfg
echo data/themes/default.cfg
