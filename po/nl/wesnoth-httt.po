#, fuzzy
msgid "The Bay of Pearls"
msgstr ""
"#-#-#-#-#  wesnoth-httt.po (Battle for Wesnoth 0.8.4-CVS)  #-#-#-#-#\n"
"#-#-#-#-#  wesnoth.po (Battle for Wesnoth 0.8.2-CVS)  #-#-#-#-#\n"
"De Parelbaai"

#: data/scenarios/Heir_To_The_Throne/Bay_of_Pearls.cfg:26
