#: data/campaigns/Liberty/scenarios/01_The_Raid.cfg:322
msgid "Remald"
msgstr "Remald"

#. [event]
#: data/campaigns/Liberty/scenarios/01_The_Raid.cfg:323
msgid "Wolmas"
msgstr "Volmas"

#. [event]
#: data/campaigns/Liberty/scenarios/01_The_Raid.cfg:328
